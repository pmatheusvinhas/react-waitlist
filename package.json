{
  "name": "react-waitlist",
  "version": "0.1.4-beta.1",
  "description": "A customizable waitlist component for React that integrates with Resend audiences",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "predeploy-storybook": "npm run build-storybook && npx msw init storybook-static --save && npx cpy-cli .nojekyll storybook-static",
    "deploy-storybook": "gh-pages -d storybook-static",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "waitlist",
    "resend",
    "email",
    "audience",
    "component",
    "form"
  ],
  "author": "Paulo M. Vinhas",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-a11y": "^8.6.4",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/addon-links": "^8.6.4",
    "@storybook/react": "^8.6.4",
    "@storybook/react-webpack5": "^8.6.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^19.0.10",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "gh-pages": "^6.3.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.3",
    "msw-storybook-addon": "^2.0.4",
    "react-docgen-typescript": "^2.2.2",
    "rollup": "^2.79.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^8.6.4",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmatheusvinhas/react-waitlist.git"
  },
  "bugs": {
    "url": "https://github.com/pmatheusvinhas/react-waitlist/issues"
  },
  "homepage": "https://pmatheusvinhas.github.io/react-waitlist",
  "changelog": "https://github.com/pmatheusvinhas/react-waitlist/blob/main/CHANGELOG.md",
  "msw": {
    "workerDirectory": [
      "public",
      "storybook-static"
    ]
  },
  "dependencies": {
    "resend": "^4.1.2"
  }
}
